class Solution
{
    //Function to sort the given linked list using Merge Sort.
    static Node mergeSort(Node head)
    {
        // add your code here
        Node h1 = head;
        ArrayList<Integer> al = new ArrayList<>();
        int index=0;
        while(h1!=null){
            al.add(h1.data);
            h1=h1.next;
            index++;
        }
        Collections.sort(al);
        Node h2 = head;
        int i=0;
       while(h2!=null){
           h2.data = al.get(i);
           h2=h2.next;
           i++;
       } 
       return head;
    }
}
