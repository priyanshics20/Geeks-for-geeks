    {
        this.data = data;
        next = prev = null;
    }
}
*/
class LinkedList
{
    //Function to sort the given doubly linked list using Merge Sort.
    static Node sortDoubly(Node head)
    {
        // add your code here
        Node h1 = head;
        ArrayList<Integer> al = new ArrayList<>();
        while(h1!=null){
            al.add(h1.data);
            h1=h1.next;
        }
        Collections.sort(al);
        Node h2=head;
        int i=0;
        while(h2!=null){
            h2.data = al.get(i);
            i++;
            h2 = h2.next;
        }
        return head;
    }
}
